{
  "name": "namisushi-api",
  "version": "2.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "removeBuild": "find ./src -name \"*.js\" -type f -delete",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
    "dev": "npm run removeBuild && nodemon --watch './**/*.ts' --exec 'ts-node' src/main.ts",
    "checkConfig": "npm run build && node -r module-alias/register tools/pre-commit/config.js && npm run removeBuild"
  },
  "author": "QuisEgoSum",
  "license": "MIT",
  "dependencies": {
    "ajv": "8.8.2",
    "ajv-errors": "3.0.0",
    "ajv-keywords": "5.1.0",
    "bcrypt": "5.0.1",
    "email-validator": "2.0.4",
    "fastify": "3.24.1",
    "fastify-cookie": "5.4.0",
    "fastify-cors": "6.0.2",
    "fastify-helmet": "5.3.2",
    "fastify-multipart": "5.3.1",
    "fastify-static": "4.5.0",
    "fastify-swagger": "4.12.6",
    "module-alias": "2.2.2",
    "mongodb": "4.2.1",
    "mongoose": "6.1.0",
    "mv": "2.1.1",
    "openapi-error": "0.0.7",
    "pino": "7.5.1",
    "socket.io": "4.4.0",
    "uuid": "8.3.2",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/mv": "^2.1.2",
    "@types/mocha": "^9.0.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@typescript-eslint/typescript-estree": "^5.6.0",
    "eslint": "^8.4.1",
    "json2md": "^1.12.0",
    "markdown-table-prettify": "^3.6.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@common": "src/common",
    "@core": "src/core",
    "@app": "src/app",
    "@utils": "src/utils",
    "@libs": "src/libs",
    "@logger": "src/core/logger",
    "@config": "src/core/config",
    "@error": "src/core/error"
  }
}
